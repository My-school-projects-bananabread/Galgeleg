<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.s195458.galgeleg">

    <!-- tilladelser -->

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mine"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.s195458.galgeleg.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.s195458.galgeleg.MainMenu" android:screenOrientation="portrait"/>
        <activity android:name="com.s195458.galgeleg.Help" android:screenOrientation="portrait"/>
        <activity android:name="com.s195458.galgeleg.Game" android:screenOrientation="portrait"/>
        <activity android:name="com.s195458.galgeleg.HighscoreScreen" android:screenOrientation="portrait"/>
        <activity android:name="com.s195458.galgeleg.PreGameScreen" android:screenOrientation="portrait"/>
        <activity android:name="com.s195458.galgeleg.GameoverScreen" android:screenOrientation="portrait"/>

    </application>

</manifest>